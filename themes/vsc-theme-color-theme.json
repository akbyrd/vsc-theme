	"tokenColors": [
		{ // Control Flow
			"scope": [
				"keyword.control",
				"entity.other.attribute-name.pragma.preprocessor",
				"punctuation.definition.directive",
			],
			"settings": { "foreground": "#E91E63", },
		},
		{ // Types
			"scope": [
				"entity.name.type",
				"entity.name.operator.type",
				"storage.type.built-in",
				//"storage.type.primitive",
			],
			"settings": { "foreground": "#009688", },
		},
		{ // Other Keywords
			"scope": [
				"keyword.operator.functionlike",
				"keyword.operator.wordlike",
				"keyword.other",
				"punctuation.section.attribute",
				"storage",
				"support.other.attribute",
			],
			"settings": { "foreground": "#2196F3", },
		},
		{ // Literals
			"scope": [
				"constant",
				"keyword.other.unit",
				"keyword.operator.minus.exponent",
				"keyword.operator.plus.exponent",
				"punctuation.definition.string",
				"string",
			],
			"settings": { "foreground": "#FFC107", },
		},
		{ // Escape Sequences
			"scope": "constant.character.escape",
			"settings": { "foreground": "#FF9800", },
		},
		{ // Format Specifiers
			"scope": "constant.other.placeholder",
			"settings": { "foreground": "#00BCD4", },
		},
		{ // Functions
			"scope": [
				"entity.name.operator",
				"entity.name.function.call",
				"entity.name.function.definition",
				"entity.name.function.member",
				"entity.name.function.operator",
				"keyword.other.operator.overload",
			],
			"settings": { "foreground": "#9AE22A", },
		},
		{ // Function Parameters
			"scope": [
				"entity.name.other.preprocessor",
				"variable.other.macro",
				"variable.parameter",
				"punctuation.vararg-ellipses",
				//"meta.function.preprocessor.parameters",
			],
			"settings": { "foreground": "#008DA0", "fontStyle": "italic"},
		},
		{ // Variables
			"scope": [
				"variable",
				"support.type.property-name",
			],
			"settings": { "foreground": "#00BCD4", },
		},
		{ // Comments
			"scope": [
				"comment",
				"punctuation.definition.comment",
			],
			"settings": { "foreground": "#5D7F30", },
		},
		{ // Macros
			"scope": "entity.name.function.preprocessor",
			"settings": { "foreground": "#8C66FF", },
		},
		{ // Enum Members
			"scope": "variable.other.enummember",
			"settings": { "foreground": "#AECF83", },
		},
		{ // Namespaces
			"scope": "entity.name.namespace",
			"settings": { "foreground": "#007C70", },
		},
		{ // Labels
			"scope": "entity.name.label",
			"settings": { "foreground": "#607D8B", },
		},
		{ // Everything Else
			"scope": [
				"keyword.operator.arithmetic",
				"keyword.operator.assignment",
				"keyword.operator.bitwise",
				"keyword.operator.comparison",
				"keyword.operator.logical",
				"keyword.operator.ternary",
				"meta",
				"punctuation.definition",
				"punctuation.section",
				"punctuation.separator",
				"punctuation.terminator",
				"storage.modifier.pointer",
				"storage.modifier.reference",
			],
			"settings": { "foreground": "#BBBBBB", },
		},
		{ // Find Missing Colors
			"scope": [
				"source",
				"class.defaultLibrary",
				"type.defaultLibrary",
				"support.class",
				"support.type",
			],
			"settings": { "foreground": "#FF00FF", },
		},
	]
}

// TODO: See if standard libary coloring works in C++

// BUG: Exclusions aren't supported: "storage" needs to exclude "storage.modifier.pointer" and "storage.modifier.reference"
// BUG: Can't tell the difference between throw exception specifier and control flow
// BUG: Can't tell between overloaded new and non-overloaded new
// BUG: "keyword.other.operator.overload" doesn't override "keyword"
// BUG: "storage.type.primitive.cpp" doesn't override "storage"
// BUG: operator"" is semantic "stringLiteral"
// BUG: Function definitions are semantic "function" which is mapped to "entity.name.function.call"
// BUG: ... in catch is "meta.parens"
// BUG: Compiler extensions like __restrict are not recognized ("source")
