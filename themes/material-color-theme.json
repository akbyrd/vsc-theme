	"semanticHighlighting": true,
	"semanticTokenColors": {
		"namespace": "#9e9e9e",
	},
	"tokenColors": [
		{
			// Macros
			"scope": [
				"entity.name.function.preprocessor",
				"entity.name.other.preprocessor.macro",
				"entity.name.variable.preprocessor",

				"meta.preprocessor.toml",
				"punctuation.definition.meta.preprocessor.toml",
			],
			"settings": { "foreground": "#7e57c2", },
		},
		{
			// Control Flow
			"scope": [
				"keyword.control",
				"keyword.preprocessor",
				"punctuation.definition.directive",

				"entity.other.attribute-name.pragma.preprocessor.cpp",

				"keyword.other.await.cs",
				"keyword.other.using.cs",
				"punctuation.separator.hash.cs",

				"meta.import.ts keyword.control.as.ts",
				"keyword.operator.expression.in.ts",
				"keyword.operator.expression.of.ts",
			],
			"settings": { "foreground": "#e91e63", },
		},
		{
			// Labels
			"scope": [
				"entity.name.label",
			],
			"settings": { "foreground": "#607d8b", },
		},
		{
			// Keywords
			"scope": [
				"entity.name.tag",
				"entity.other.attribute",
				"keyword.other",
				"storage",

				"keyword.operator.functionlike.cpp",
				"keyword.operator.wordlike.cpp",
				"meta.head.function.definition.cpp keyword.control.throw.cpp",
				"storage.type.template.argument.typename",
				"storage.type.template.argument.class",

				"keyword.query",

				"keyword.control.as.ts",
				"keyword.control.default.ts",
				"keyword.control.export.ts",
				"keyword.operator.expression",
				"keyword.operator.new.ts",

				"fenced_code.block.language.markdown",
				"meta.separator.markdown",
				"text.html.markdown punctuation.definition",
				"text.html.markdown punctuation.separator.table.markdown",
				"text.html.markdown markup.heading.setext",

				"source.css entity.other",

				"punctuation.line.separator.math.tex",
				"punctuation.math.operator.latex",
			],
			"settings": { "foreground": "#2196f3", },
		},
		{
			// Types
			"scope": [
				"entity.name.operator.type",
				"entity.name.type",
				"keyword.other.var",
				"keyword.type",
				"storage.type.built-in",
				"support.class",
				"support.type",
				"variable.language.super",

				"entity.other.inherited-class.ts",
				"keyword.operator.expression.void.ts",
				"meta.import.ts variable.other",
				"support.variable.ts",
			],
			"settings": { "foreground": "#00897b", },
		},
		{
			// Functions
			"scope": [
				"entity.name.function",
				"entity.name.operator",
				"support.function",

				"keyword.operator.new.cpp",

				"punctuation.definition.function.math.tex",
			],
			"settings": { "foreground": "#64dd17", },
		},
		{
			// Function Parameters
			"scope": [
				"variable.other.macro",
				"variable.parameter",

				"punctuation.vararg-ellipses.variable.parameter.preprocessor.cpp",
			],
			"settings": { "foreground": "#00bcd4", "fontStyle": "italic"},
		},
		{
			// Variables
			"scope": [
				"entity.name.variable",
				"support.type.property-name",
				"support.variable",
				"variable",

				"storage.type.template.argument",

				"entity.name.label.ts",
				"meta.object-literal.key.ts",

				"meta.array.toml",

				"meta.function.math.tex",
			],
			"settings": { "foreground": "#00bcd4", },
		},
		{
			// Literals
			"scope": [
				"constant",
				"keyword.other.unit",
				"punctuation.definition.string",
				"string",
				"support.constant",
				"variable.other.enummember",

				"keyword.operator.minus.exponent",
				"keyword.operator.plus.exponent",

				"entity.name.variable.enum-member.cs",

				"text.xml",

				"text.html",
				"text.html string source.css",

				"source.css punctuation.definition.constant",
			],
			"settings": { "foreground": "#ffc107", },
		},
		{
			// Escape Sequences
			"scope": [
				"constant.character.escape",

				"constant.other.character-class.regexp",
				"keyword.control.anchor.regexp",
				"keyword.operator.negation.regexp",
				"keyword.operator.quantifier.regexp",
				"punctuation.definition.character-class.regexp",
				"constant.character.numeric.regexp",
			],
			"settings": { "foreground": "#ff9800", },
		},
		{
			// Format Specifiers
			"scope": [
				"constant.other.placeholder",

				"punctuation.definition.interpolation",

				"keyword.operator.or.regexp",
				"keyword.other.back-reference.regexp",
				"punctuation.definition.group.assertion.regexp",
				"punctuation.definition.group.no-capture.regexp",
				"punctuation.definition.group.regexp",
				"variable.other.regexp",
			],
			"settings": { "foreground": "#f44336", },
		},
		{
			// Comments
			"scope": [
				"comment",
				"punctuation.definition.comment",
			],
			"settings": { "foreground": "#33691e", }, // TODO: Try 424242
		},
		{
			// Documentation Tags
			"scope": [
				"storage.type.class.doxygen.cpp",

				"entity.name.tag.cs",
				"comment.documentation.name.cs",
				"punctuation.definition.tag.cs",

				"comment.block.documentation keyword.other.parameter.direction",
			],
			"settings": { "foreground": "#2196f377", "fontStyle": "italic", },
		},
		{
			// Documentation Parameters
			"scope": [
				"comment.line.double-slash.documentation variable.parameter",
				"comment.block.documentation variable.parameter",
			],
			"settings": { "foreground": "#00bcd499", "fontStyle": "italic", },
		},
		{
			// Everything Else
			"scope": [
				"entity.name.namespace",
				"entity.name.type.namespace",
				"punctuation",
				"source",

				"storage.modifier.pointer.cpp",
				"storage.modifier.reference.cpp",

				"keyword.other.attribute-specifier.cs",

				"keyword.other.hashtable.begin.powershell",

				"meta.tag.xml",

				"storage.type.function.arrow.ts",
				"constant.language.import-export-all.ts",
				"meta.namespace.declaration.ts entity.name.type.module.ts",

				"text.html.markdown",
				"text.html.markdown constant.other.reference",
				"text.html.markdown string.other.link",

				"meta.function.math.tex punctuation.definition",
			],
			"settings": { "foreground": "#9e9e9e", },
		},
		{
			// DEBUG
			"scope": [
			],
			"settings": { "foreground": "#ff0000", },
		},
	]
}
